 public boolean isOptionSelected( String expectedValue, String indx) {
	    WebElement dropdownElement = driver.findElement(By.xpath("(//select[@style='width: 100%; height: 56px; background-color: white; padding: 8px;'])["+indx+"]"));
        Select select = new Select(dropdownElement);
        String selectedText = select.getFirstSelectedOption().getText().trim();
        txt= selectedText;
        return selectedText.equalsIgnoreCase(expectedValue);
    }
