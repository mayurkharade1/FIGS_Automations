package com.figs.automation.runners;

import org.openqa.selenium.*;
import org.openqa.selenium.support.ui.ExpectedConditions;
import org.openqa.selenium.support.ui.Select;
import org.openqa.selenium.support.ui.WebDriverWait;

import java.text.SimpleDateFormat;
import java.time.Duration;
import java.util.Calendar;
import java.util.List;

public class Validation {

    private final WebDriver driver;

    // data for this row
    private List<String> arl;
    private final String[] Arr2 = new String[2]; // [0]=status, [1]=message
    private String txt = "";                     // hold UI text value
    private String txt1 = ""; 
    private String txt2 = ""; 
    private String txt3 = ""; 
    
    
    public Validation(WebDriver driver) {
        this.driver = driver;
    }

    public void AcctServiceValidations(List<String> rowData) throws InterruptedException {
        this.arl = rowData;
        String txnType = arl.get(1);
        if ("Onus_Deposit_AEPS".equalsIgnoreCase(txnType) || "Deposit_Account".equalsIgnoreCase(txnType))
        {
        	TransactionsChkResult(); 	
       }else if("Onus_Withdrawal_AEPS".equalsIgnoreCase(txnType) || "Withdrawal_Account".equalsIgnoreCase(txnType))
       {
    	   TransactionsChkResult();
       }else if("Onus_FundTransfer_AEPS".equalsIgnoreCase(txnType) || "FundTransfer_Account".equalsIgnoreCase(txnType))
       {
    	   TransactionsChkResult();
       }else if("Onus_Mini_Statement_AEPS".equalsIgnoreCase(txnType) || "Mini_Statement_Account".equalsIgnoreCase(txnType))
       {
    	   TransactionsChkResult();
       }else if("Onus_Balance_Enquiry_AEPS".equalsIgnoreCase(txnType) || "Balance_Enquiry_Account".equalsIgnoreCase(txnType))
       {
    	   TransactionsChkResult();
       }else if("AadhaarSeeding".equalsIgnoreCase(txnType))
       {
    	   AdhaarSeedingDeseedingChkResult();
       }
        
        
    }
    
    public void AccountOpeningValidations(List<String> rowData) throws InterruptedException {
        this.arl = rowData;
        String txnType = arl.get(1);
        if ("Account Opening".equalsIgnoreCase(txnType)|| "RDSTDR".equalsIgnoreCase(txnType))
        {
        	AccountOpeningChkResult();
       }
    }
    
    public void AcuqisitonServiceValidations(List<String> rowData) throws InterruptedException {
        this.arl = rowData;

        String txnType = arl.get(1);

         if ("MoneyTransfer_CIF".equalsIgnoreCase(txnType) || "MoneyTransfer_ACC".equalsIgnoreCase(txnType) || "DepositToLoanAccount".equalsIgnoreCase(txnType) || "View Loan Data Collection".equalsIgnoreCase(txnType)  ) {
        	 MoneyTransferandLoanDepositChkResult();
        } else if("PMJJBY".equalsIgnoreCase(txnType) || "PMSBY".equalsIgnoreCase(txnType) || "APY".equalsIgnoreCase(txnType))
        {
        	SSSChkResult();
        }
        else if("Lead Creation".equalsIgnoreCase(txnType) ||"ATMCardBlock_ACC".equalsIgnoreCase(txnType) || "ATMCardBlock_Card".equalsIgnoreCase(txnType) || "Stop Payment of Cheque".equalsIgnoreCase(txnType) || "Cheque Book Payment".equalsIgnoreCase(txnType) )
        {
        	EaseBankingChkResult();
        }
        else if("AadhaarSeeding_SubKO".equalsIgnoreCase(txnType))
        {
        	
        }
         else  {
            System.out.println("No matching validation flow found for type: " + txnType);
        }
    }


    // Transactions check result
    private void TransactionsChkResult() throws InterruptedException {
        TransactionsMessageReader();
        String timestamp = new SimpleDateFormat("dd/MM/yyyy h:mm:ss a")
                .format(Calendar.getInstance().getTime());

        if (arl.size() > 7 && arl.get(7).equals(Arr2[0])) {
            Arr2[0] = "Pass : " + timestamp;
        } else {
            Arr2[0] = "Fail : " + timestamp;
        }

        System.out.println("Status is: " + Arr2[0]);
        System.out.println("Actual Result is: " + Arr2[1]);
        Result.setResult(Arr2[0],Arr2[1]);
    }

    
   
    //  Transactions Validation
    private void TransactionsMessageReader() {
        try {
            if ("AEPS".equals(arl.get(6)) && ValidateAEPSTransactionsPage()) {
                Arr2[1] = "User is navigated to " + txt + " page.";
                Arr2[0] = "Pass";
            }else if(arl.get(5).equals("ValidData") && ValidateDATATransactionsPage(arl.get(4))){
				Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0]= "Pass";
			}else if ("Account".equals(arl.get(6)) && ValidateAccountTransactionsPage()) {
                Arr2[1] = "User is navigated to " + txt + " page.";
                Arr2[0] = "Pass";
            } else if ("PROCEED_DISABLED".equals(arl.get(6)) && ValidatePROCEEDTransactionsPage()) {
                Arr2[1] = "Proceed button is disabled.";
                Arr2[0] = "Pass";
            } else if ("PROCEED_ENABLED".equals(arl.get(6)) && ValidatePROCEEDButtonIsEnabled()) {
                Arr2[1] = "Proceed button is enabled and clickable.";
                Arr2[0] = "Pass";
            } else if ("PROCEED_CONFIRM".equals(arl.get(6)) && ValidatePROCEED_CONFIRMButtonIsEnabled()) {
                Arr2[1] = "Proceed button is enabled and clickable.";
                Arr2[0] = "Pass";
            } else if ("PROCEED_PopUp".equals(arl.get(6)) && ValidatePROCEEDButtonPopUp()) {
                Arr2[1] = "Proceed button is enabled and clickable.";
                Arr2[0] = "Pass";
            } else if ("PROCEED_FIG".equals(arl.get(6)) && ValidatePROCEEDButtonFig()) {
                Arr2[1] = "Proceed button is enabled and clickable. user is able to do fingerprint auth";
                Arr2[0] = "Pass";
            } else if ("YES, I DO".equals(arl.get(6)) && ClickYesIDoButton()) {
                Arr2[1] = "'YES, I DO' button is enabled and clicked.";
                Arr2[0] = "Pass";
            } else if ("visually impaired".equals(arl.get(6)) && ClickCheckBox()) {
                Arr2[1] = "Customer is visually impaired checkBox button is enabled and clicked.";
                Arr2[0] = "Pass";
            } else if ("Finger".equals(arl.get(6)) && FingerPrint()) {
                Arr2[1] = "Customer FingerPrint Authentication Done and Proceed Button is Clickable";
                Arr2[0] = "Pass";
            } else if ("CusmFinger".equals(arl.get(6)) && CusmFingerPrint()) {
                Arr2[1] = "Customer FingerPrint Authentication Done and Proceed Button is Clickable";
                Arr2[0] = "Pass";
            } else if ("Pop-Up".equals(arl.get(6)) && isTransactionSuccessful()) {
                Arr2[1] = "Transaction successful and Got Success message Pop-Up";
                Arr2[0] = "Pass";
            } else if ("BankName".equals(arl.get(6)) && ValidateSBIBANKTransactionsPage()) {
                Arr2[1] = "Valid Data is Entered '" + txt + "'.";
                Arr2[0] = "Pass";
            } else if(arl.get(6).equals("AadhaarNumber") || arl.get(6).equals("VID") && ValidateAADHAARNOTransactionsPage())
			{
				Arr2[1] = "Valid Data is Entered 'AadhaarNumber'.";
				Arr2[0] = "Pass";
			}else if(arl.get(6).equals("Beneficiary Aadhaar Number") && ValidateSecondAADHAARNOTransactionsPage())
			{
				Arr2[1] = "Valid Data is Entered 'AadhaarNumber'.";
				Arr2[0] = "Pass";
			}else if(arl.get(6).equals("Witness Aadhaar Number") && ValidateThirdAADHAARNOTransactionsPage())
			{
				Arr2[1] = "Valid Data is Entered 'AadhaarNumber'.";
				Arr2[0] = "Pass";
			}else {
                Arr2[0] = "Fail";
                Arr2[1] = "Test Case Failed";
            }
        } catch (Exception e) {
            System.out.println("Exception Message: " + e.getMessage());
            try {
                Arr2[0] = "Fail";
                Arr2[1] = driver.findElement(By.xpath("//div[@class='userinfo']/font")).getText();
            } catch (Exception ex) {
                Arr2[0] = "Fail";
                Arr2[1] = (Arr2[1] == null ? "" : Arr2[1]) +
                        " /n Error caused due to unexpected flow or negative testcase. Check Screenshot.";
            }
        }
    }
    
 // AEPS Transactions check result
    private void AdhaarSeedingDeseedingChkResult() throws InterruptedException {
    	AdhaarSeedingDeseedingMessageReader();
        String timestamp = new SimpleDateFormat("dd/MM/yyyy h:mm:ss a")
                .format(Calendar.getInstance().getTime());

        if (arl.size() > 7 && arl.get(7).equals(Arr2[0])) {
            Arr2[0] = "Pass : " + timestamp;
        } else {
            Arr2[0] = "Fail : " + timestamp;
        }

        System.out.println("Status is: " + Arr2[0]);
        System.out.println("Actual Result is: " + Arr2[1]);
        Result.setResult(Arr2[0],Arr2[1]);
    }

    
   
    // AEPS Transactions Validation
    private void AdhaarSeedingDeseedingMessageReader() {
        try {
        	if ( arl.get(6).equals("Agent Aadhaar Seeding") && isClickedAgentAdhaarSeeding() )
			{
				 Arr2[1] = "Selected User Service is = " + txt;
				 Arr2[0]="Pass";
			}
			 else if( arl.get(6).equals("AadhaarSeeding") && isClickedAdhaarSeeding() )
			{
				Arr2[1] = "Selected User Service is = " + txt;
				Arr2[0]="Pass";
			}
			 else if( arl.get(6).equals("AadhaarSeeding") && isClickedAdhaarDeSeeding() )
			{
				Arr2[1] = "Selected User Service is = " + txt;
				Arr2[0]="Pass";
			}
			else if (arl.get(6).equals("EnterAdhaarNumber") && isClickedAdhaarNumber())
			{
				Arr2[1] = "User Entered Adhar Number = " + txt;
				Arr2[0]="Pass";	
			}
			else if (arl.get(6).equals("Enter Account Number") && isClickedAccountNumber())
			{
				Arr2[1] = "User Entered Account Number = " + txt;
				Arr2[0]="Pass";	
			}
			else if (arl.get(6).equals("ClickSubmit"))
			{
				Arr2[1] = "Proceed button is enable and clicked";
				Arr2[0]="Pass";	
			}
			else if (arl.get(6).equals("SuccessfulAuthorize"))
			{
				Arr2[1] = "Proceed button is Succesful Authorize";
				Arr2[0]="Pass";	
			}
			else if (arl.get(6).equals("ClickProceed"))
			{
				Arr2[1] = "Proceed PopUP button is enable and clicked";
				Arr2[0]="Pass";	
			}
			else if(arl.get(6).equals("YES, I DO") && ClickYesIDoButton())
			{
				Arr2[1] = "'YES, I DO' button is enabled and clicked.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(6).equals("Finger") && FingerPrint())   
			{
				Arr2[1] = "Customer FingerPrint Authentication Done and Proceed Button is Clickable";
				Arr2[0] = "Pass";
			}
			else if(arl.get(6).equals("TransactionStatus") && waitForTransactionReceipt(txt) )
			{
				Arr2[1]= txt;   
				Arr2[0]= "Pass";
			}
			 else if( arl.get(6).equals("Yes") && isClickedYes() )
			{
				Arr2[1] = "Selected User Service is = " + txt;
				Arr2[0]="Pass";
			}
			 else if( arl.get(6).equals("BankNameAdharSeeding") && isClickedSBI() )
			{
				Arr2[1] = "Bank Name is Selected  ";
				Arr2[0]="Pass";
		    }
			
			else if(arl.get(6).equals("ClickReset"))
			{
				Arr2[1] =  "Data is Cleared";
			}
           else {
                Arr2[0] = "Fail";
                Arr2[1] = "Test Case Failed";
            }
        } catch (Exception e) {
            System.out.println("Exception Message: " + e.getMessage());
            try {
                Arr2[0] = "Fail";
                Arr2[1] = driver.findElement(By.xpath("//div[@class='userinfo']/font")).getText();
            } catch (Exception ex) {
                Arr2[0] = "Fail";
                Arr2[1] = (Arr2[1] == null ? "" : Arr2[1]) +
                        " /n Error caused due to unexpected flow or negative testcase. Check Screenshot.";
            }
        }
    }
    
    
    
    
    
    private void MoneyTransferandLoanDepositChkResult() throws InterruptedException {
    	TrxnMessageReaderMoneyTransferandLoanDepositChkResult();
        String timestamp = new SimpleDateFormat("dd/MM/yyyy h:mm:ss a")
                .format(Calendar.getInstance().getTime());

        if (arl.size() > 7 && arl.get(6).equals(Arr2[0])) {
            Arr2[0] = "Pass : " + timestamp;
        } else {
            Arr2[0] = "Fail : " + timestamp;
        }

        System.out.println("Status is: " + Arr2[0]);
        System.out.println("Actual Result is: " + Arr2[1]);
        Result.setResult(Arr2[0],Arr2[1]);
    }
    
    public void TrxnMessageReaderMoneyTransferandLoanDepositChkResult() throws ElementNotInteractableException
	{
		try
		{
			if((arl.get(5).equals("EnterDepositorName") && ValidateDATATransactionsPage(arl.get(4))) || (arl.get(5).equals("EnterAddress")&& ValidateDATATransactionsPage(arl.get(4)) )|| (arl.get(5).equals("EnterAmount") && ValidateDATATransactionsPage(arl.get(4))) )
			{
				Arr2[1] = txt+ " is filled";
				Arr2[0] = "Pass";
			}
			else if(( arl.get(5).equals("DeposittoLoanAccount") && isLoanDepositPageOpen() ))
			{
				Arr2[1] = txt+ " is open";
				Arr2[0]="Pass";
			}
			else if((arl.get(5).equals("MoneyTransfer") && ValidateMoneyTransferPage() ))
			{
				Arr2[1] = "user is navigated "+txt+" page";
				Arr2[0] = "Pass";
			}
			else if((arl.get(5).equals("ClickSubmit") &&  ValidateProceedButtonMoneyTransfer()))
			{
				Arr2[1] = "Proceed button is enabled and clickable.";
				Arr2[0] = "Pass";
			}
			else if((arl.get(5).equals("EnterAdhaarNumber") && isIdentificationNumberFilled()))
			{
				Arr2[1] = "Adhaar Number"+txt+" Entered.";
				Arr2[0] = "Pass";
			}
			else if((arl.get(5).equals("EnterMobileNumber") && ValidateDATATransactionsPage(arl.get(4)) ))
			{
				Arr2[1] = "Mobile Number "+txt+" Entered.";
				Arr2[0] = "Pass";
			}
			else if((arl.get(5).equals("ClickVerificationCode") && isProceedPopUpVisible()  ))
			{
				Arr2[1] = txt;
				Arr2[0] = "Pass";
			}
			else if((arl.get(5).equals("ProceedPopUp") && isGreenTickVisible() ))
			{
				Arr2[1] = "Number Verified and Ready to proceed";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("ClickConfirm") && isAgentAuthenticationPopupVisible() )
			{
				Arr2[1] = txt+" visible";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("SuccessfulAuthorize") && isTransactionRecieptPageVisible())
			{
				Arr2[1] = "Receipt of "+txt+" Transaction Visible";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("select CIF") &&  selectCIFRadioButton()  )
			{
				Arr2[1] = "User Selected "+txt+" Transaction ";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("select ACC") &&  selectACCRadioButton() ) 
			{
				Arr2[1] = "User Selected "+txt+" Transaction ";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Enter Account Number") &&  ValidateDATATransactionsPage(arl.get(4)) ) 
			{
				Arr2[1] = "Account Number"+txt+" Entered.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Check Benificiary Acc Validation") && isBeneficiaryAccountNumberSelected() && isBeneficiaryAccountHolderNameFilled() &&  isBranchCodeFilled() && isAddressLine1Filled() )
			{
				Arr2[1] = " Benificiary Account Details "+txt+" "+txt1+" "+txt2+" "+txt3;
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Check CIF Validation")&& getValidationMessageForCIF() )
			{

				Arr2[1] ="User got validation red tool tip "+txt;
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Enter CIF") && ValidateDATATransactionsPage(arl.get(4)))
			{
				Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0]= "Pass";
			}
			else if(arl.get(5).equals("TransactionStatus") && WaitForTransactionReceipt())
			{
				Arr2[1]= "Successful";
				Arr2[0]= "Pass";
			}
			/*LAMS*/
			 else if(arl.get(5).equals("ViewLoanDataCollection") && isClickedViewLoanDataCollection() )
			{
				Arr2[1] = "Selected User Service is = " + txt;
				Arr2[0]="Pass";
			}
			else if(arl.get(5).equals("Customer Name") && ValidateDATATransactionsPage() )
			{
				Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0]= "Pass";
			}
			else if(arl.get(5).equals("Account Number") && ValidateDATATransactionsPage() )
			{
				Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0]= "Pass";
			}
			else if(arl.get(5).equals("ClickSearch") && ValidateTOSearch())
			{
				Arr2[1] = "Search button is clickable.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Details") && ValidateTODetails())
			{
				Arr2[1] = "Details button is clickable.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Follow Up") && ValidateTOFollowUP())
			{
				Arr2[1] = "Follow Up button is clickable.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Contact Mode") && isOptionSelected(arl.get(4),"1"))
			{
				Arr2[1] = txt + " is Selected";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Date Follow Up") && ValidateDATATransactionsPage() )
			{
				Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0]= "Pass";
			}

			else if(arl.get(5).equals("Contacted Person") && isOptionSelected(arl.get(4),"2"))
			{
				Arr2[1] = txt + " is Selected";
				Arr2[0] = "Pass";
			}
