public boolean ValidateServiceATMBlock(String expectedServiceName) {
	 List<WebElement> services = driver.findElements(By.xpath(
		        "//a[text()='ATM Card Block Service' or text()='Cheque Book Request' or text()='Stop Payment of Cheque']"
		    ));

		    for (WebElement service : services) {
		        String classes = service.getAttribute("class"); // e.g., "nav-link active"
		        txt = service.getText().trim();

		        if (classes.contains("active")) {
		            System.out.println("Active Service: " + txt);
		            return txt.equalsIgnoreCase(expectedServiceName); // true if matches expected
		        }
		    }

		    System.out.println("No active service found.");
		    return false;
   }
  
public  boolean isRadioButtonSelectedAndStore() {
    try {
        List<WebElement> radioButtons = driver.findElements(By.name("option"));
        for (WebElement radio : radioButtons) {
            if (radio.isSelected()) {
               txt = radio.getAttribute("id");
                return true;
            }
        }
    } catch (Exception e) {
    	
        System.out.println(e.getMessage());
    }
    return false;
}



/*Account Opening method*/

public boolean isAccordionButtonClicked(String value, String index) {
    try {
        WebDriverWait wait = new WebDriverWait(driver, Duration.ofSeconds(10));
        
     
        WebElement button = wait.until(ExpectedConditions.presenceOfElementLocated(
            By.xpath("(//h2[@class='accordion-header']//button[p[text()='" + value + "']])[" + index + "]")
        ));
        
        // Scroll into view with an offset so its not hidden under sticky header
        ((JavascriptExecutor) driver).executeScript(
            "arguments[0].scrollIntoView(true); window.scrollBy(0, -100);", button
        );

        // Wait until clickable (no overlay, visible, enabled)
        wait.until(ExpectedConditions.elementToBeClickable(button));

        boolean selected = button.isSelected();
        txt = button.getText();

        // Click only if not already expanded
        if (!selected) {
            try {
                button.click();
            } catch (ElementClickInterceptedException e) {
                // Fallback: JavaScript click if normal click fails
                ((JavascriptExecutor) driver).executeScript("arguments[0].click();", button);
            }
            selected = !selected;
        }
        return selected;

    } catch (Exception e) {
        System.out.println("Accordion click failed: " + e.getMessage());
        return false;
    }
}






public boolean checkBoxSelectedOrNot(String index)
{
	
	WebElement checkBox = driver.findElement(By.xpath("(//input[@type='checkbox'])["+index+"]"));
	boolean checkboxClicked =checkBox.isSelected();
	if(!checkboxClicked)
	{
		checkBox.click();
		checkboxClicked = !checkboxClicked ;
	}
	return checkboxClicked;
}



public boolean validateAndInputIfEnabled(String expectedValue) {
    WebElement inputField = driver.findElement(By.xpath("//input[@placeholder=\"Enter Mother's Middle Name\" ]"));
    boolean isEnabled = inputField.isEnabled();
    txt = inputField.getAttribute("value");

    if (isEnabled) {
        inputField.clear();
        inputField.sendKeys(expectedValue);
        txt= inputField.getAttribute("value");
        return true; // Value entered successfully
    } else if (!isEnabled && expectedValue.equalsIgnoreCase(txt)) {
        return true; // Disabled but already has correct value
    }

    return false; // Neither condition passed
}

public boolean isCustomerDetailsSectionVisible() {
    try {
        WebDriverWait wait = new WebDriverWait(driver, Duration.ofSeconds(15));

        WebElement customerDetailsHeader = wait.until(ExpectedConditions.visibilityOfElementLocated(
            By.xpath("//button[@class='accordion-button' and @aria-expanded='true']//p[text()='Customer Details']")
        ));

        return customerDetailsHeader.isDisplayed();
    } catch (TimeoutException e) {
        System.out.println("Timed out waiting for Customer Details section to become visible.");
        return false;
    } catch (Exception e) {
        System.out.println("Error while checking Customer Details visibility: " + e.getMessage());
        return false;
    }
}

/*Validating Save button is enebled and clickable(Arindam Harh ,12/08/2025)*/

public boolean ValidateButtonIsEnabled(String text) {
	
	// Check for screen-level proceed button
	try{
		WebElement validBtn = driver.findElement(By.xpath("//button[.//p[text()='"+text+"']]"));
		txt = validBtn.getText();
	 boolean isenabled = validBtn.isEnabled();
		if (isenabled) {
			validBtn.click();
			
			return true;		
	}
	}catch(Exception e)
	{}	
	return false;
}

