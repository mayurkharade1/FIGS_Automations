public boolean isClickedAdhaarNumber()
{
	WebElement clickadharseeding=driver.findElement(By.xpath("//input[@placeholder='Enter Identification Number' or @placeholder='Enter Aadhaar Number']"));
	
	boolean clickcheck=clickadharseeding.isDisplayed();
	if(clickcheck)
	{
		txt=clickadharseeding.getAttribute("value");
		return true;
	}
	else return false;
}



/*Adhaae seding deseeding*/


public boolean isClickedAgentAdhaarSeeding() 
{
    
	WebElement agentAdhaarSeeding = driver.findElement(By.xpath("(//a[@role='button' and contains(text(), 'Aadhaar Seeding') and contains(@class, 'nav-link')])[1]"));

    if (agentAdhaarSeeding.isDisplayed()) {
        txt=agentAdhaarSeeding.getText();
        return true;
    } else {
        
        return false;

  } 
}

public boolean isClickedAdhaarDeSeeding()
{
	WebElement clickadharDeseeding=driver.findElement(By.xpath("//p[@class='CardLabel' and contains(normalize-space(), 'Aadhaar De-Seeding')]"));
	
	boolean clickcheck=clickadharDeseeding.isDisplayed();
	if(clickcheck)
	{
		txt=clickadharDeseeding.getText();
		return true;
	}
	else return false;
}

public boolean isClickedAccountNumber()
{
	WebElement clickadharseeding=driver.findElement(By.xpath("//input[@placeholder='Enter Account Number']"));
	
	boolean clickcheck=clickadharseeding.isDisplayed();
	if(clickcheck)
	{
		txt=clickadharseeding.getAttribute("value");
		return true;
	}
	else return false;
}

public boolean isClickedYes() {
    
	WebElement clickYes = driver.findElement(By.xpath("//div[@class='css-1dimb5e-singleValue' and text()='YES']"));

    if (clickYes.isSelected()) {
        txt=clickYes.getText();
        return true;
    } else {
        
        return false;

  } 
}

public boolean isClickedSBI() {
 	WebElement clickSBI = driver.findElement(By.xpath("//option[@value='000607261']"));
// 	By.xpath("//p[contains(text(), 'Previous Bank Name')]")
// 	
     if (clickSBI.isDisplayed()) {
         txt=clickSBI.getText();
         return true;
     } else {
         
         return false;

   } 
}

private void waitForSpinnerToDisappear(WebDriver driver) {
    By spinner = By.xpath("//div[@class='spinner-border' and @role='status']");

    try {
        // Very short wait (0.5s) just to see if spinner exists
        WebDriverWait shortWait = new WebDriverWait(driver, Duration.ofMillis(1000));
        boolean appeared = shortWait.until(d -> !driver.findElements(spinner).isEmpty());

        if (appeared) {
            // If it appeared, wait max 10s for disappearance
            new WebDriverWait(driver, Duration.ofSeconds(20)).until(ExpectedConditions.invisibilityOfElementLocated(spinner));
        }
    } catch (TimeoutException te) {
    }
}
}
