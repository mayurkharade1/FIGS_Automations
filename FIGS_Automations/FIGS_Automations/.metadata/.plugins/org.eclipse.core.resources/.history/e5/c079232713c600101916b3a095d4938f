				Arr2[0] = "Pass";
			}
		 else if (arl.get(5).equals("EnterAdhaarNumber") && isClickedAdhaarNumber())
		{
				Arr2[1] = "User Entered Adhar Number = " + txt;
				Arr2[0]="Pass";	
		}
		 else if(arl.get(5).equals("Finger") && FingerPrint())   
		{
				Arr2[1] = "Customer FingerPrint Authentication Done and Proceed Button is Clickable";
				Arr2[0] = "Pass";
		}
		 else if (arl.get(5).equals("ClickProceed"))
		{
				Arr2[1] = "Proceed PopUP button is enable and clicked";
				Arr2[0]="Pass";	
		}
		else if(arl.get(5).equals("YES, I DO") && ClickYesIDoButton())
		{
				Arr2[1] = "'YES, I DO' button is enabled and clicked.";
				Arr2[0] = "Pass";
		}
