			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Address2") && handleInputField(arl.get(6),arl.get(4),arl.get(12)))
			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
