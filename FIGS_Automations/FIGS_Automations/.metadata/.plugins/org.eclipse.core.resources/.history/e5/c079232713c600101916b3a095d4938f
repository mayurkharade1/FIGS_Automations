			else if(arl.get(5).equals("SuccessfulAuthorize") )
			{
				Arr2[1] ="Proceed button is enabled and clickable.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("YES, I DO") && ClickYesIDoButton())
			{
				Arr2[1] = "'YES, I DO' button is enabled and clicked.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Finger") && FingerPrint())   
			{
				Arr2[1] = "Customer FingerPrint Authentication Done and Proceed Button is Clickable";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("PROCEED_PopUp") && ValidatePROCEEDButtonPopUp())
			{
				Arr2[1] = "Proceed button is enabled and clickable.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("TransactionStatus") && waitForTransactionReceipt(txt) )
			{
				Arr2[1]= txt;   
				Arr2[0]= "Pass";
			}
			else if(arl.get(5).equals("ChequeType") && ValidateDATATransactionsPage())
			{
				Arr2[1] = "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0] = "Pass";
			}
			else if( arl.get(5).equals("Reason for Stop Payment") && ValidateDATATransactionsPage())
			{
				Arr2[1] = "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("FROMChequeNumber") && ValidateFromChequeNumber() )
			{
				Arr2[1] = "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("TOChequeNumber") && ValidateTOChequeNumber() )
			{
				Arr2[1] = "Valid data '" + txt + "' is Entered/Selected in the focused field.";
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("EnterAmount") && ValidateDATATransactionsPage()  )
			{
			Arr2[1]= "Valid data '" + txt + "' is Entered/Selected in the focused field.";
			Arr2[0]= "Pass";
			}
			else
			{
				Arr2[1] = " /n Error Caused due to some un expected Error or due to conditions of Negative testcase. Check Screenhot for clarity";
				Arr2[0]="Fail";
			}
			
		}
		
		catch(Exception e1)
		{
			Arr2[0] = "Fail";
			Arr2[1] = " /n Error Caused due to some un expected Error or due to conditions of Negative testcase.";
		}
	}
    
    
    
    
    private void SSSChkResult() throws InterruptedException {
    	TrxnMessageReaderSSS();
        String timestamp = new SimpleDateFormat("dd/MM/yyyy h:mm:ss a")
                .format(Calendar.getInstance().getTime());

        if (arl.size() > 7 && arl.get(6).equals(Arr2[0])) {
            Arr2[0] = "Pass : " + timestamp;
        } else {
            Arr2[0] = "Fail : " + timestamp;
        }

        System.out.println("Status is: " + Arr2[0]);
        System.out.println("Actual Result is: " + Arr2[1]);
        Result.setResult(Arr2[0],Arr2[1]);
    }

