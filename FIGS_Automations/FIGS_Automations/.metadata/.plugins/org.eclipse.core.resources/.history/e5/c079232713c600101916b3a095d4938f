			{
				Arr2[1] = txt + " is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("NomineeAge") && handleInputField(arl.get(6),arl.get(4),arl.get(12)) )
			{
				Arr2[1] = txt + " is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("EnterNomineeMobile") && handleInputField(arl.get(6),arl.get(4),arl.get(12)) )
			{
				Arr2[1] = txt + " is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("NomineeNameOnPassbook") && isAtmCardRadioSelected(arl.get(6),"3"))
			{
				Arr2[1] =txt+ " is selected/Entered";			
				Arr2[0] = "Pass"; 
			}
			else if(arl.get(5).equals("LinkwithSHG") && isAtmCardRadioSelected(arl.get(6),"4"))
			{
				Arr2[1] =txt+ " is selected/Entered";			
				Arr2[0] = "Pass"; 
			}
			else
			{
				Arr2[0] = "Fail";
				Arr2[1] = " /n Error Caused due to some un expected Error or due to conditions of Negative testcase. Check Screenhot for clarity";
			}

		}
		catch(NoAlertPresentException ex)
		{
			
		}
   		}
    
    
    
    
    
    
    
