		{
			 Arr2[1] = "Selected User Service is = adhaar sub KO ";
			 Arr2[0]="Pass";
		}
		 else if( arl.get(5).equals("AadhaarSeeding") && isClickedAdhaarSeeding() )
		{
			Arr2[1] = "Selected User Service is = " + txt;
			Arr2[0]="Pass";
		}
		 else if(arl.get(5).equals("SubKO_ID") && selectSubKORadioButton()  )
		{
				Arr2[1] = "User Selected "+txt+" Radio Button ";
				Arr2[0] = "Pass";
		}
