public boolean ValidateokProceedPopup()
{
	
	WebElement okButton = driver.findElement(By.xpath("//h5[normalize-space()='Try Again']"));
	boolean tryagain=okButton.isDisplayed();

	if (tryagain) {
		OkPopUP(); 
		return true;
	} else {
	    System.out.println(" eKYC button is not clicked/active.");
	    return false;
	}

}

public void OkPopUP() {
    try {
        WebDriverWait wait = new WebDriverWait(driver, Duration.ofSeconds(15));
        WebElement okButton = wait.until(ExpectedConditions.visibilityOfElementLocated(
            By.xpath("//button[.//p[contains(text(), 'OK')]]")));

        if (okButton.isDisplayed()) {
            okButton.click();
        }
    } catch (Exception e) {}
}

