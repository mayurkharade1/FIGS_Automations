public boolean waitForTransactionReceipt(String expectedMessage) {
    try {
        WebElement txnStatusMsg = driver.findElement(By.xpath("//p[text()='" + expectedMessage + "']"));
        if (txnStatusMsg.isDisplayed()) {
            txt = txnStatusMsg.getText();  // You may already have 'txt' declared elsewhere
            return true;
        }
    } catch (Exception e) {
        // Optional: log the exception if needed
    }
    return false;
}

public boolean dropdownfieldgetvalue(String index)
		{
			WebElement dropdown = driver.findElement(By.xpath("(//select[@class='select-dropdown'])[" + index + "]"));
			if (dropdown.isDisplayed()) {
	            Select select = new Select(dropdown);
	            txt = select.getFirstSelectedOption().getText();  // Same global variable txt
	            return true;
	        }
			else return false;
		}	

