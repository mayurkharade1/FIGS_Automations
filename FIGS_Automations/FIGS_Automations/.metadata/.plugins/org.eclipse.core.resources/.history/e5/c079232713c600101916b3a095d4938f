			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("State_eKYC") && handleInputField(arl.get(6),arl.get(4),arl.get(12)))
			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("District") && handleInputField(arl.get(6),arl.get(4),arl.get(12)))
			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Country_eKYC") && handleInputField(arl.get(6),arl.get(4),arl.get(12)))
			{
				Arr2[1] = txt+" is selected/Entered";			
				Arr2[0] = "Pass";
			}
			else if(arl.get(5).equals("Pincode_eKYC") && handleInputField(arl.get(6),arl.get(4),arl.get(12)))
			{
